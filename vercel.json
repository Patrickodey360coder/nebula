{
  "version": 2,
  "builds": [
    {
      "src": "api/*.tss",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["GET", "POST"],
      "dest": "api/$1.js",
      "headers": {
        "Cache-Control": "no-store",
        "Pragma": "no-cache",
        "Referrer-Policy": "no-referrer"
      }
    }
  ]
}
